<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.TecFresh">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.TecFresh.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.TecFresh.userTabLogin"/>

        <activity android:name="com.example.TecFresh.userTabRegister"/>

        <activity android:name=".customerVerify"/>

        <activity android:name=".shopkeeperVerify"/>

        <activity android:name=".customerResetPassword"/>

        <activity android:name=".customerMain"/>

        <activity android:name="shopkeeperMain"/>

        <activity android:name=".shopkeeperResetPassword"/>

    </application>

</manifest>